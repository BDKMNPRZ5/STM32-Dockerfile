{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build Ubuntu",
			"command": "docker",
			"args": [
				"build",
				"-t",
				"jasonyangee/stm32_ubuntu:latest",
				"-f",
				"Dockerfile.ubuntu",
				"."
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": []
		},
		{
			"type": "shell",
			"label": "Build Alpine",
			"command": "docker",
			"args": [
				"build",
				"-t",
				"jasonyangee/stm32_alpine:latest",
				"-f",
				"Dockerfile.alpine",
				"."
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": []
		}
	]
}
